[ignore]
.*/build/.*

[include]

[libs]
./node_modules/@polkadot/dev/flow-typed

[options]
module.name_mapper='^@polkadot/client-chains\(.*\)$' -> '<PROJECT_ROOT>/packages/client-chains/src\1'
module.name_mapper='^@polkadot/client-p2p\(.*\)$' -> '<PROJECT_ROOT>/packages/client-p2p/src\1'
module.name_mapper='^@polkadot/client-rpc\(.*\)$' -> '<PROJECT_ROOT>/packages/client-rpc/src\1'
module.name_mapper='^@polkadot/client-wasm\(.*\)$' -> '<PROJECT_ROOT>/packages/client-wasm/src\1'
